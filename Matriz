#include<stdio.h>
#include<stdlib.h>

int main()
{
	int matriz [5][5];
	int linha, coluna;
	
	for(linha = 0; linha < 5; linha++)
	{
		for(coluna = 0; coluna < 5; coluna++){
			printf("Adcione elementos na matriz [%d][%d]: ", linha, coluna);
			scanf("%d", &matriz[linha][coluna]);
		}
	}
	
	printf("\n\nMatriz: \n\n");
	for(linha = 0; linha < 5; linha++){
		if(linha != 0)
		{
			printf("\n");
		}
		for(coluna = 0; coluna < 5; coluna++)
			{
				printf(" %d  ", matriz[linha][coluna]);
		    }
		    
	}
	
	
	
	
	return 0;
}
